{
  "name": "@crackle/cli",
  "version": "0.12.6",
  "license": "MIT",
  "author": "SEEK",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "crackle": "bin.js"
  },
  "files": [
    "CHANGELOG.md",
    "bin.js",
    "config",
    "dist"
  ],
  "scripts": {
    "build": "crackle package",
    "dev": "crackle dev",
    "prepublishOnly": "tsx ../../scripts/tag-bootstrap.ts",
    "postversion": "tsx ../../scripts/update-bootstrap.ts"
  },
  "dependencies": {
    "@crackle/core": "^0.30.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/yargs": "^17.0.32"
  }
}
