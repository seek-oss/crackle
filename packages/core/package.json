{
  "name": "@crackle/core",
  "version": "0.12.0",
  "main": "dist/crackle-core.cjs.js",
  "module": "dist/crackle-core.esm.js",
  "exports": {
    "./dev": {
      "module": "./dev/dist/crackle-core-dev.esm.js",
      "default": "./dev/dist/crackle-core-dev.cjs.js"
    },
    "./fix": {
      "module": "./fix/dist/crackle-core-fix.esm.js",
      "default": "./fix/dist/crackle-core-fix.cjs.js"
    },
    "./build": {
      "module": "./build/dist/crackle-core-build.esm.js",
      "default": "./build/dist/crackle-core-build.cjs.js"
    },
    ".": {
      "module": "./dist/crackle-core.esm.js",
      "default": "./dist/crackle-core.cjs.js"
    },
    "./serve": {
      "module": "./serve/dist/crackle-core-serve.esm.js",
      "default": "./serve/dist/crackle-core-serve.cjs.js"
    },
    "./start": {
      "module": "./start/dist/crackle-core-start.esm.js",
      "default": "./start/dist/crackle-core-start.cjs.js"
    },
    "./logger": {
      "module": "./logger/dist/crackle-core-logger.esm.js",
      "default": "./logger/dist/crackle-core-logger.cjs.js"
    },
    "./package": {
      "module": "./package/dist/crackle-core-package.esm.js",
      "default": "./package/dist/crackle-core-package.cjs.js"
    },
    "./route-data": {
      "module": "./route-data/dist/crackle-core-route-data.esm.js",
      "default": "./route-data/dist/crackle-core-route-data.cjs.js"
    },
    "./resolve-config": {
      "module": "./resolve-config/dist/crackle-core-resolve-config.esm.js",
      "default": "./resolve-config/dist/crackle-core-resolve-config.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Ben",
  "license": "MIT",
  "files": [
    "/build",
    "/dev",
    "/dist",
    "/entries",
    "/fix",
    "/index",
    "/logger",
    "/mocks",
    "/package",
    "/resolve-config",
    "/route-data",
    "/serve",
    "/start"
  ],
  "scripts": {
    "sync-dependencies": "sync-dependencies --source vite"
  },
  "preconstruct": {
    "entrypoints": [
      "build.ts",
      "dev.ts",
      "fix.ts",
      "index.ts",
      "logger.ts",
      "package.ts",
      "resolve-config.ts",
      "route-data.ts",
      "serve.ts",
      "start.ts"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/plugin-syntax-typescript": "^7.14.5",
    "@crackle/babel-plugin-remove-exports": "0.1.0",
    "@crackle/router": "0.1.2",
    "@vanilla-extract/babel-plugin": "^1.1.2",
    "@vanilla-extract/css": "^1.7.3",
    "@vanilla-extract/integration": "^5.0.1",
    "@vanilla-extract/vite-plugin": "^3.3.1",
    "@vitejs/plugin-react": "^2.1.0",
    "builtin-modules": "^3.2.0",
    "chalk": "^4.0.0",
    "ensure-gitignore": "^1.2.0",
    "esbuild": "^0.15.9",
    "eval": "^0.1.8",
    "express": "^4.17.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.0.0",
    "ink": "^3.0.9",
    "mlly": "^0.5.16",
    "pretty-ms": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-node-externals": "^4.1.1",
    "serialize-javascript": "^6.0.0",
    "serve-handler": "^6.1.3",
    "sort-package-json": "^1.57.0",
    "used-styles": "^2.2.0",
    "vite": "^3.2.0-0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.15",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.11.57",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/serialize-javascript": "^5.0.1",
    "@types/serve-handler": "^6.1.1",
    "ink-testing-library": "^2.1.0",
    "memfs": "^3.4.7",
    "strip-ansi": "^7.0.1",
    "sync-dependencies": "^1.0.4",
    "type-fest": "^2.18.0"
  },
  "peerDependencies": {
    "typescript": "~4.7.4"
  }
}
