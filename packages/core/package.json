{
  "name": "@crackle/core",
  "version": "0.14.0",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./build": {
      "types": "./dist/build.d.ts",
      "import": "./dist/build.mjs",
      "require": "./dist/build.cjs"
    },
    "./clean": {
      "types": "./dist/clean.d.ts",
      "import": "./dist/clean.mjs",
      "require": "./dist/clean.cjs"
    },
    "./dev": {
      "types": "./dist/dev.d.ts",
      "import": "./dist/dev.mjs",
      "require": "./dist/dev.cjs"
    },
    "./fix": {
      "types": "./dist/fix.d.ts",
      "import": "./dist/fix.mjs",
      "require": "./dist/fix.cjs"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.mjs",
      "require": "./dist/logger.cjs"
    },
    "./package": {
      "types": "./dist/package.d.ts",
      "import": "./dist/package.mjs",
      "require": "./dist/package.cjs"
    },
    "./resolve-config": {
      "types": "./dist/resolve-config.d.ts",
      "import": "./dist/resolve-config.mjs",
      "require": "./dist/resolve-config.cjs"
    },
    "./route-data": {
      "types": "./dist/route-data.d.ts",
      "import": "./dist/route-data.mjs",
      "require": "./dist/route-data.cjs"
    },
    "./serve": {
      "types": "./dist/serve.d.ts",
      "import": "./dist/serve.mjs",
      "require": "./dist/serve.cjs"
    },
    "./start": {
      "types": "./dist/start.d.ts",
      "import": "./dist/start.mjs",
      "require": "./dist/start.cjs"
    },
    "./entries/*": "./entries/*",
    "./mocks/*": "./mocks/*",
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "author": "SEEK",
  "files": [
    "./dist",
    "./entries",
    "./mocks",
    "./CHANGELOG.md"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "unbuild --stub",
    "sync-dependencies": "sync-dependencies --source vite && prettier --write package.json"
  },
  "wireit": {
    "build": {
      "command": "unbuild",
      "files": [
        "./src/**/*.{ts,tsx}"
      ]
    }
  },
  "preconstruct": {
    "entrypoints": [
      "build.ts",
      "clean.ts",
      "dev.ts",
      "fix.ts",
      "index.ts",
      "logger.ts",
      "package.ts",
      "resolve-config.ts",
      "route-data.ts",
      "serve.ts",
      "start.ts"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@crackle/babel-plugin-remove-exports": "0.1.1",
    "@crackle/router": "^0.2.1",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/integration": "^6.0.0",
    "@vanilla-extract/vite-plugin": "^3.6.1",
    "@vitejs/plugin-react": "^2.2.0",
    "builtin-modules": "^3.3.0",
    "chalk": "^4.1.2",
    "ensure-gitignore": "^1.2.0",
    "es-module-lexer": "^1.1.0",
    "esbuild": "^0.15.9",
    "eval": "^0.1.8",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "ink": "^3.2.0",
    "pretty-ms": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve-from": "^5.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-node-externals": "^5.0.2",
    "serialize-javascript": "^6.0.0",
    "serve-handler": "^6.1.5",
    "sort-package-json": "^1.57.0",
    "type-fest": "^3.2.0",
    "used-styles": "^2.4.1",
    "vite": "^3.2.4"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.20",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/serialize-javascript": "^5.0.2",
    "@types/serve-handler": "^6.1.1",
    "ink-testing-library": "^2.1.0",
    "memfs": "^3.4.11",
    "strip-ansi": "^7.0.1",
    "sync-dependencies": "^1.0.4",
    "typescript": "*"
  },
  "peerDependencies": {
    "typescript": "~4.7.4"
  }
}
