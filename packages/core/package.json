{
  "name": "@crackle/core",
  "version": "0.17.0",
  "main": "dist/crackle-core.cjs.js",
  "module": "dist/crackle-core.esm.js",
  "exports": {
    "./dev": {
      "module": "./dev/dist/crackle-core-dev.esm.js",
      "default": "./dev/dist/crackle-core-dev.cjs.js"
    },
    "./fix": {
      "module": "./fix/dist/crackle-core-fix.esm.js",
      "default": "./fix/dist/crackle-core-fix.cjs.js"
    },
    "./build": {
      "module": "./build/dist/crackle-core-build.esm.js",
      "default": "./build/dist/crackle-core-build.cjs.js"
    },
    "./clean": {
      "module": "./clean/dist/crackle-core-clean.esm.js",
      "default": "./clean/dist/crackle-core-clean.cjs.js"
    },
    ".": {
      "module": "./dist/crackle-core.esm.js",
      "default": "./dist/crackle-core.cjs.js"
    },
    "./serve": {
      "module": "./serve/dist/crackle-core-serve.esm.js",
      "default": "./serve/dist/crackle-core-serve.cjs.js"
    },
    "./start": {
      "module": "./start/dist/crackle-core-start.esm.js",
      "default": "./start/dist/crackle-core-start.cjs.js"
    },
    "./logger": {
      "module": "./logger/dist/crackle-core-logger.esm.js",
      "default": "./logger/dist/crackle-core-logger.cjs.js"
    },
    "./package": {
      "module": "./package/dist/crackle-core-package.esm.js",
      "default": "./package/dist/crackle-core-package.cjs.js"
    },
    "./route-data": {
      "module": "./route-data/dist/crackle-core-route-data.esm.js",
      "default": "./route-data/dist/crackle-core-route-data.cjs.js"
    },
    "./resolve-config": {
      "module": "./resolve-config/dist/crackle-core-resolve-config.esm.js",
      "default": "./resolve-config/dist/crackle-core-resolve-config.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "author": "SEEK",
  "files": [
    "/build",
    "/clean",
    "/dev",
    "/dist",
    "/entries",
    "/fix",
    "/index",
    "/logger",
    "/mocks",
    "/package",
    "/resolve-config",
    "/route-data",
    "/serve",
    "/start"
  ],
  "scripts": {
    "sync-dependencies": "sync-dependencies --source vite && prettier --write package.json"
  },
  "preconstruct": {
    "entrypoints": [
      "build.ts",
      "clean.ts",
      "dev.ts",
      "fix.ts",
      "index.ts",
      "logger.ts",
      "package.ts",
      "resolve-config.ts",
      "route-data.ts",
      "serve.ts",
      "start.ts"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/plugin-syntax-typescript": "^7.20.0",
    "@crackle/babel-plugin-remove-exports": "0.1.1",
    "@crackle/router": "^0.2.1",
    "@ungap/structured-clone": "^1.0.1",
    "@vanilla-extract/css": "^1.9.2",
    "@vanilla-extract/integration": "^6.0.1",
    "@vanilla-extract/vite-plugin": "^3.7.0",
    "@vitejs/plugin-react": "^3.0.0",
    "builtin-modules": "^3.3.0",
    "chalk": "^4.1.2",
    "ensure-gitignore": "^1.2.0",
    "es-module-lexer": "^1.1.0",
    "esbuild": "^0.16.3",
    "eval": "^0.1.8",
    "express": "^4.18.2",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "glob-to-regexp": "^0.4.1",
    "ink": "^3.2.0",
    "pretty-ms": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resolve-from": "^5.0.0",
    "rollup": "^3.7.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-node-externals": "^5.0.2",
    "semver": "^7.3.8",
    "serialize-javascript": "^6.0.0",
    "serve-handler": "^6.1.5",
    "sort-package-json": "^1.57.0",
    "type-fest": "^3.2.0",
    "used-styles": "^2.4.1",
    "vite": "^4.0.3"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.20",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/semver": "^7.3.13",
    "@types/serialize-javascript": "^5.0.2",
    "@types/serve-handler": "^6.1.1",
    "ink-testing-library": "^2.1.0",
    "memfs": "^3.4.11",
    "strip-ansi": "^7.0.1",
    "sync-dependencies": "^1.0.4",
    "typescript": "*"
  },
  "peerDependencies": {
    "typescript": ">=4.9.4"
  }
}
