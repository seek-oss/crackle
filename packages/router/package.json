{
  "name": "@crackle/router",
  "version": "0.2.0",
  "main": "dist/crackle-router.cjs.js",
  "module": "dist/crackle-router.esm.js",
  "exports": {
    ".": {
      "module": "./dist/crackle-router.esm.js",
      "default": "./dist/crackle-router.cjs.js"
    },
    "./routes": {
      "module": "./routes/dist/crackle-router-routes.esm.js",
      "default": "./routes/dist/crackle-router-routes.cjs.js"
    },
    "./server": {
      "module": "./server/dist/crackle-router-server.esm.js",
      "default": "./server/dist/crackle-router-server.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Ben",
  "license": "MIT",
  "files": [
    "/dist",
    "/routes",
    "/server",
    "valid-routes.d.ts"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts",
      "routes.ts",
      "server.ts"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-router-dom": "6.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.21"
  }
}
