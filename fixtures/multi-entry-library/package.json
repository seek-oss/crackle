{
  "name": "@crackle-fixtures/multi-entry-library",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.cjs.d.ts",
  "files": [
    "/components",
    "/dist",
    "/extras"
  ],
  "author": "Ben",
  "license": "MIT",
  "dependencies": {
    "@crackle/router": "0.1.2",
    "braid-design-system": "file:../../.yalc/braid-design-system",
    "react": "17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@crackle/cli": "0.5.2",
    "@crackle/core": "0.7.0",
    "@types/react-dom": "^17"
  },
  "scripts": {
    "build": "crackle package",
    "serve": "crackle serve",
    "start": "crackle start"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.js.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.js.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components": {
      "import": {
        "types": "./components/dist/index.js.d.ts",
        "default": "./components/dist/index.mjs"
      },
      "require": {
        "types": "./components/dist/index.js.d.ts",
        "default": "./components/dist/index.js"
      }
    },
    "./extras": {
      "import": {
        "types": "./extras/dist/index.js.d.ts",
        "default": "./extras/dist/index.mjs"
      },
      "require": {
        "types": "./extras/dist/index.js.d.ts",
        "default": "./extras/dist/index.js"
      }
    }
  }
}
